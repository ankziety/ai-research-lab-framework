{
  "ode_test_cases": {
    "harmonic_oscillator": {
      "equation": "d\u00b2x/dt\u00b2 = -\u03c9\u00b2x",
      "omega": 1.0,
      "initial_conditions": [
        1.0,
        0.0
      ],
      "analytical_solution": "x(t) = cos(\u03c9t)",
      "time_span": [
        0,
        6.283185307179586
      ],
      "expected_final_state": [
        1.0,
        0.0
      ]
    },
    "exponential_decay": {
      "equation": "dy/dt = -\u03bby",
      "lambda": 0.5,
      "initial_conditions": [
        1.0
      ],
      "analytical_solution": "y(t) = exp(-\u03bbt)",
      "time_span": [
        0,
        2.0
      ],
      "expected_final_state": [
        0.36787944117144233
      ]
    },
    "pendulum": {
      "equation": "d\u00b2\u03b8/dt\u00b2 = -(g/L)sin(\u03b8)",
      "g": 9.81,
      "L": 1.0,
      "initial_conditions": [
        0.1,
        0.0
      ],
      "analytical_solution": "\u03b8(t) \u2248 \u03b8\u2080cos(\u221a(g/L)t)",
      "time_span": [
        0,
        2.0
      ],
      "expected_period": 2.006
    }
  },
